<% include ../../partials/header %>

<div class="container ">
    <div class="pmdata">
            <div class="table-responsive-lg">
            <table class="table table-bordered">
                <thead>
                    <tr class="d-flex">
                        <th class="col-sm-12 header3" >Check Activity</th>
                    </tr>

                    <tr class="d-flex">
                        <th class="col-sm-3 header4" ><%= data.metaData[0].name %></th>
                        <th class="col-sm-3 header4" ><%= data.metaData[1].name %></th>
                        <th class="col-sm-3 header4" ><%= data.metaData[2].name %></th>
                        <th class="col-sm-3 header4" ><%= data.metaData[3].name %></th>
                        <th class="col-sm-3 header4" ><%= data.metaData[4].name %></th>
                    </tr>
                </thead>

                <tbody>
                    <% for (let i of data.rows) { %>
                        <tr class="d-flex">
                            <th class="col-sm-3 rowtext" > <%= i.USERNAME %>  </th>
                            <th class="col-sm-3 rowtext" > <%= i.SID %>  </th>
                            <th class="col-sm-3 rowtext" > <%= i.OSUSER %>  </th>
                            <th class="col-sm-3 rowtext" > <%= i.SQL_ID %>  </th>
                            <th class="col-sm-3 rowtext" > <%= i.SQL_TEXT %>  </th>
                        </tr>
                    <% } %>
                </tbody>
            </table>
        </div>
    </div>
 
<br><br>


<hr>
</div>

<% include ../../partials/footer %>